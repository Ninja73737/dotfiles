{
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "go",
    "html",
    "javascript",
    "markdown",
    "python",
    "rust",
    "svelte",
    "typescript"
  ],
  "cSpell.language": "en-GB",
  "cSpell.ignoreRegExpList": [
    "[A-Z]{3}\\d{3,}([A-Z]|\\d)*",
    "\\\\\\w+((\\[|\\{)[\\\\\\w\\=, ]+(\\}|\\]))*",
    "]\\(.+\\)",
    "^\\s*\\w+: ",
    "```[^`]*```",
    "`[^\\n`]+`"
  ],
  "cSpell.enabledLanguageIds": [
    "csharp",
    "go",
    "json",
    "markdown",
    "rmarkdown",
    "pandoc",
    "php",
    "plaintext",
    "python",
    "rmd",
    "tex",
    "vimwiki",
    "yaml",
    "yml"
  ],
  "cSpell.userWords": [
    "CERB",
    "L'HÃ´pital's",
    "Lemieux",
    "Maclaurin",
    "Toohey",
    "analyzed",
    "analyzes",
    "analyzing",
    "apophenia",
    "astroglia",
    "centered",
    "collateralized",
    "confounder",
    "confounders",
    "counterintuitively",
    "disincentivized",
    "docstrings",
    "doctest",
    "dyscalculia",
    "explainability",
    "extremum",
    "furloughed",
    "glia",
    "incongruent",
    "invertibility",
    "jitteriness",
    "mochi",
    "obscura",
    "phineas",
    "popsicle",
    "potentiation",
    "reconsolidation",
    "supremum",
    "telos",
    "triangularizable",
    "universalizable",
    "voluntariness",
    "worldview"
  ],
  "diagnostic-languageserver.mergeConfig": true,
  "diagnostic-languageserver.filetypes": {
    "json": [],
    "markdown": [],
    "pandoc": [],
    "rmarkdown": []
  },
  "diagnostic-languageserver.linters": {
    "languagetool": {
      "args": [
        "-l",
        "en-CA",
        "--disable",
        "WHITESPACE_RULE,EN_QUOTES,ARROWS,MORFOLOGIK_RULE_EN_CA,DASH_RULE,COMMA_PARENTHESIS_WHITESPACE,UNLIKELY_OPENING_PUNCTUATION",
        "%file"
      ]
    }
  },
  "diagnostic.enableSign": false,
  "markdownlint.config": {
    "MD004": false,
    "MD005": false,
    "MD013": false,
    "MD022": false,
    "MD024": false,
    "MD026": false,
    "MD028": false,
    "MD029": false,
    "MD030": false,
    "MD032": false,
    "MD033": false,
    "MD036": false
  },
  "git.enableGutters": false,
  "texlab.build.onSave": true,
  "texlab.forwardSearch.executable": "/usr/bin/zathura",
  "texlab.forwardSearch.args": ["--synctex-forward", "%l:1:%f", "%p"],
  "prettier.useTabs": false,
  "prettier.tabWidth": 2
}
