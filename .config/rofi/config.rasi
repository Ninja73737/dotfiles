configuration {
  font:                  "SF Pro Rounded 13";
  icon-theme:            "oomox-Dark-Moon";
  show-icons:            true;
  drun-display-format:   "{icon}  {name}";
  display-run:           "⎇ ";
  display-drun:          "􀊫 ";
  display-window:        "􀇴 ";
  modi:                  "window,run,drun";
  lines:                 7;
  line-padding:          10;
  bw:                    0;
  padding:               0;
  separator-style:       none;
  hide-scrollbar:        true;
  line-margin:           0;
}

