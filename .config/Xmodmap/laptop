! Clear modifiers
clear control
clear lock
clear mod1
clear mod4

! Originally Control_L
keycode 37 = Caps_Lock NoSymbol Caps_Lock
! Originally Alt_L
keycode 64 = Super_L NoSymbol Super_L
! Originally Caps_Lock
keycode 66 = Control_L NoSymbol Control_L
! Originally Super_L
keycode 133 = Alt_L Meta_L Alt_L Meta_L

! Set modifiers
add control = Control_L
add lock = Caps_Lock
add mod1 = Alt_L
add mod4 = Super_L
