{
  "diagnostic.enableSign": false,
  "markdownlint.config": {
    "MD005": false,
    "MD013": false,
    "MD026": false,
    "MD028": false,
    "MD029": false,
    "MD030": false,
    "MD033": false,
    "MD036": false
  },
  "git.addedSign.hlGroup": "DiffAdd",
  "git.removedSign.hlGroup": "DiffDelete",
  "git.changedSign.hlGroup": "DiffChange",
  "git.changeRemovedSign.hlGroup": "DiffChange",
  "git.topRemovedSign.hlGroup": "DiffDelete",
  "git.conflict.incoming.hlGroup": "WarningMsg",
  "git.conflict.current.hlGroup": "Error",
  "python.linting.pep8Enabled": true,
  "python.linting.pep8Args": ["--max-line-length=100"],
  "cSpell.ignoreRegExpList": [
    "[A-Z]{3}\\d{3,}([A-Z]|\\d)*",
    "\\\\\\w+((\\[|\\{)[\\\\\\w\\=, ]+(\\}|\\]))*",
    "]\\(.+\\)",
    "^\\s*\\w+: ",
    "```[^`]*```",
    "`[^\\n`]+`"
  ],
  "latex.build.onSave": true,
  "latex.forwardSearch.executable": "zathura",
  "latex.forwardSearch.args": ["%p"],
  "cSpell.language": "en-GB",
  "cSpell.enabledLanguageIds": [
    "csharp",
    "go",
    "json",
    "markdown",
    "rmarkdown",
    "pandoc",
    "php",
    "plaintext",
    "rmd",
    "tex",
    "vimwiki",
    "yml"
  ],
  "diagnostic-languageserver.mergeConfig": true,
  "diagnostic-languageserver.filetypes": {
    "json": [],
    "markdown": [],
    "pandoc": [],
    "rmarkdown": []
  },
  "diagnostic-languageserver.linters": {
    "languagetool": {
      "args": [
        "-l",
        "en-CA",
        "--disable",
        "WHITESPACE_RULE,EN_QUOTES,ARROWS,MORFOLOGIK_RULE_EN_CA,DASH_RULE,COMMA_PARENTHESIS_WHITESPACE,UNLIKELY_OPENING_PUNCTUATION",
        "%file"
      ]
    }
  },
  "cSpell.userWords": [
    "CERB",
    "Confounders",
    "L'HÃ´pital's",
    "Lemieux",
    "Popsicle",
    "Toohey",
    "Voluntariness",
    "analyzed",
    "analyzes",
    "analyzing",
    "centered",
    "confounder",
    "disincentivized",
    "doctest",
    "explainability",
    "invertibility",
    "supremum",
    "triangularizable"
  ],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "args": ["--log-file=/tmp/ccls.log", "-v=1"],
      "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
      "rootPatterns": [".ccls", "compile_commands.json"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        },
        "client": {
          "snippetSupport": true
        }
      }
    }
  }
}
